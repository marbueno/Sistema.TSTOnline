@{
    ViewData["Title"] = "ESTOQUES";
}

<div class="row justify-content-center">
    <div class="btn-group">
        <button class="btn btn-danger" onClick="backToHome()"><i class="fa fa-undo fa-lg"></i> Voltar</button>
    </div>
</div>

<div class="table-responsive">
    <table id="tblEstoque" class="table table-striped table-bordered table-hover">
        <thead class="text-uppercase">
            <tr>
                <th data-column-id="id" data-type="numeric">Código</th>
                <th width="20%" data-column-id="sku">SKU</th>
                <th width="50%" data-column-id="nome">Nome</th>
                <th width="10%" data-column-id="qtde">Qtde</th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>

</div>

@section Scripts{
    <script src="~/js/estoque/estoque.js"></script>
}